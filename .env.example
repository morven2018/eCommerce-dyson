# Commercetools API Configuration
CTP_PROJECT_KEY=your_project_key
CTP_CLIENT_ID=your_client_id
CTP_CLIENT_SECRET=your_client_secret
CTP_AUTH_URL=https://auth.{region}.gcp.commercetools.com
CTP_API_URL=https://api.{region}.gcp.commercetools.com

# Space-separated scopes (keep on one line)
CTP_SCOPES=view_categories:{projectKey} manage_my_orders:{projectKey}

# Optional settings
CTP_REGION=your_region
CTP_TIMEOUT=30000

CTP_SECRET_KEY = 'your-secret-key';